{
    "name": "atelierspierrot/composer-assets-extension",
    "type": "composer-installer",
    "description": "A custom Composer installer to manage \"library-assets\" package type",
    "keywords": [ "composer", "package", "assets" ],
    "homepage": "https://github.com/atelierspierrot/composer-assets-extension",
    "version": "0.0.1-dev",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Les Ateliers Pierrot & contributors",
            "email": "contact@ateliers-pierrot.fr",
            "homepage": "http://www.ateliers-pierrot.fr/"
        },{
            "name": "Pierre Cassat",
            "email": "piero.wbmstr@gmail.com",
            "homepage": "https://github.com/PieroWbmstr"
        }
    ],
    "require": { "php": ">=5.3.0" },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/PieroWbmstr/devtest" }
    ],
    "require-dev": { "sami/sami": "dev-master" },
    "autoload": { "psr-0": { "ComposerAssetsExtension": "src" } },
    "extra": { "class": "\\ComposerAssetsExtension\\Installer\\AssetsInstaller" },
    "scripts": { "post-autoload-dump": "\\ComposerAssetsExtension\\Installer\\AssetsInstaller::postAutoloadDump" }
}
